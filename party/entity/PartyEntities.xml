<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.0.xsd">


    <entity entity-name="Party" package="party.entity">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="partyTypeEnumId" type="id"/>
<field name="createdDate" type="date-time" />
        <relationship type="one" title="PartyType" related="Enumeration">
            <key-map field-name="partyTypeEnumId"/>
        </relationship>

    </entity>


    <entity entity-name="Person" package="party.entity">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="firstName" type="text-short"/>
        <field name="lastName" type="text-short"/>
        <field name="dateOfBirth" type="date"/>
        <relationship type="one" related="Party">
            <key-map field-name="partyId"/>
        </relationship>
    </entity>


    <entity entity-name="PartyGroup" package="party.entity">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="groupName" type="text-short"/>
        <field name="description" type="text-long"/>
        <relationship type="one" related="Party">
            <key-map field-name="partyId"/>
        </relationship>
    </entity>


    <entity entity-name="ContactMech" package="party.entity">
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="contactMechTypeId" type="id"/>
        <field name="infoString" type="text-medium"/>
        <field name="createdDate" type="date-time" />
        <relationship type="one" title="ContactMechType" related="Enumeration">
            <key-map field-name="contactMechTypeEnumId"/>
        </relationship>
    </entity>


    <entity entity-name="PartyContactMech" package="party.entity">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="fromDate" type="date-time" is-pk="true"/>
        <field name="thruDate" type="date-time"/>
        <relationship type="one" related="Party">
            <key-map field-name="partyId"/>
        </relationship>
        <relationship type="one" related="ContactMech">
            <key-map field-name="contactMechId"/>
        </relationship>
    </entity>

</entities>